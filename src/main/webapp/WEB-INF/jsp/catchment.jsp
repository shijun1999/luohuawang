function addListingMarker( listing )
{
	var latLng = new google.maps.LatLng( listing["lat"], listing["lng"] );
	
	var autoOpen = false;
	if( bnds == null ) {
		bnds = new google.maps.LatLngBounds( latLng, latLng );
		autoOpen = true;
	}
	else {
		bnds.extend( latLng );
	}
	
   	var key = latLngToKey(latLng);
   	
   	var existing = listingMarkers[key];
   	if( existing ) {
   		existing.data.push( listing );
   		var allSolds = true;
   		try
   		{
   			for( var i=0; i<existing.data.length; ++i ) {
   				if( existing.data[i].status != "SOLD" ) {
   					allSolds = false;
   					break;
   				}
   			}
   		}
   		catch( e )
   		{
   		}
   		existing.setIcon( new google.maps.MarkerImage(
			getMapIcon( true, allSolds )
	   	) );
	   	var shadow = getMapIconShadow( true, allSolds );
	   	if( shadow ) {
	   		existing.setFlat( false );
	   		existing.setShadow( shadow );
	   	}
	   	
	   	listingKeysById[''+listing.listId] = [ key, existing.data.length-1 ];
   	} 
   	else {
		var marker = new google.maps.Marker( {
	   		position: latLng,
	   		map: gmap,
	   		flat: true,
	   		zIndex: 100
	   	} );
	   	marker.setIcon( getMapIcon( false, ( listing.status == "SOLD" ) ) );
	   	var shadow = getMapIconShadow( false, ( listing.status == "SOLD" ) );
	   	if( shadow ) {
	   		marker.setFlat( false );
	   		marker.setShadow( shadow );
	   	}
	   	marker.data = [ listing ];
	   	google.maps.event.addListener( marker, "click", function(e) {
	   		onMarkerClick( marker, listing );
	   	});
	   	listingMarkers[key] = marker;
	   	
	   	listingKeysById[''+listing.listId] = [ key, 0 ];
	   	
	   	if( preOpenMarker && listingMarkers.length == 1 ) {
	   		autoOpenMarker = marker;
	   	}
	}
}

function onGeocode( results, status, info )
{
	if (status == google.maps.GeocoderStatus.OK) {
 		if (status != google.maps.GeocoderStatus.ZERO_RESULTS) {
 			info["lat"] = results[0].geometry.location.lat();
 			info["lng"] = results[0].geometry.location.lng();
 			addListingMarker( info );
		}
	}
}

