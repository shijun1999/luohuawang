function geocode( info )
{
	if( geocoder ) {
		geocoder.geocode( { 
			"address": info["address"] + " " + info["city"] + " " + 
				info["provState"] + " " + info["pcode"] }, 
			function( results, status ) { 
				listingsPlaced++;
				onGeocode( results, status, info );
				checkFinished();
			}
		);
	}
	else {
		listingsPlaced++;
	}
}

function placeListings( infos )
{
	for( var i=0; i<infos.length; ++i ) {
		if( infos[i].lat && infos[i].lng ) {
			listingsPlaced++;
			addListingMarker( infos[i] );
			checkFinished();
		}
		else {
			geocode( infos[i] );
		}
	}
	
	// force it
	listingsPlaced = listingInfos.length;
	checkFinished();
}

function fitBounds()
{
	var bnds = null;
	var totalMarkers = 0;
	

	var devs = calcStandardDeviations();
	var standardLatDeviation = devs["standardLatDeviation"];
	var standardLngDeviation = devs["standardLngDeviation"];
	var meanLatDeviation = devs["meanLatDeviation"];
	var meanLngDeviation = devs["meanLngDeviation"];
	
	for( var i in listingMarkers) {
		if( typeof i == 'string' ) {
			totalMarkers ++;
			var latLng = listingMarkers[i].getPosition();
			var latDev = Math.abs(latLng.lat()-meanLatDeviation);
			var lngDev = Math.abs(latLng.lng()-meanLngDeviation)
			if(  standardLatDeviation*2 < latDev || standardLngDeviation*2 < lngDev ) {
				continue;
			}
			if( bnds == null ) {
				bnds = new google.maps.LatLngBounds( latLng, latLng );
				autoOpen = true;
			}
			else {
				bnds.extend( latLng );
			}
		}
	}
	
	if( totalMarkers == 1 ) {
		for( var i in listingMarkers) {
	        if( typeof i == 'string' ) {
	            gmap.setCenter( listingMarkers[i].getPosition() );
	            gmap.setZoom( 15 );
	            break;
	        }
		}
	}
	else if( bnds != null ) {
		gmap.fitBounds( bnds );
		if( 0 != postZoom ) {
			setTimeout( function() {
				gmap.setZoom( gmap.getZoom() + postZoom );
			}, 300 );
		}
	}
	
	if( totalMarkers == 0 ) {
		//showNoListingsFoundWindow();
	}
}

function calcStandardDeviations()
{
	var totalLat = 0;
	var totalLng = 0;
	var total = 0;
	
	var lats = [];
	var lngs = [];
	
	for( var i in listingMarkers) {
		if( typeof i == 'string' ) {
			var marker = listingMarkers[i];
			var latLng = marker.getPosition();
			var lat = latLng.lat();
			var lng = latLng.lng();
			totalLat += lat;
			totalLng += lng;
			total++;
			lats.push( lat );
			lngs.push( lng );
		}
	}
	
	if( total <= 1 ) {
		return {
			"standardLatDeviation" : 0,
			"standardLngDeviation" : 0,
			"meanLatDeviation" : 0,
			"meanLngDeviation" : 0
		};
	}
	
	var meanLat = totalLat / total;
	var meanLng = totalLng / total;
	
	var latDeviations = [];
	var lngDeviations = [];
	for( var i=0; i<lats.length; ++i ) {
		latDeviations.push( lats[i] - meanLat );
		lngDeviations.push( lngs[i] - meanLng );
	}
	
	var sumSquareLatDeviations = 0;
	var sumSquareLngDeviations = 0;
	for( var i=0; i<lats.length; ++i ) {
		sumSquareLatDeviations += latDeviations[i] * latDeviations[i];
		sumSquareLngDeviations += lngDeviations[i] * lngDeviations[i];
	}
	
	var standardLatDeviation = Math.sqrt(sumSquareLatDeviations/(total-1));
	var standardLngDeviation = Math.sqrt(sumSquareLngDeviations/(total-1));
	
	try
	{
		console.log( "standard lat deviation: " + standardLatDeviation );
		console.log( "standard lng deviation: " + standardLngDeviation );
		console.log( "===========================" );
		for( var i=0; i<lats.length; ++i ) {
			console.log( i + ": " + latDeviations[i] + "," + lngDeviations[i] );
		}
	}
	catch( e )
	{
	}
	
	return {
		"standardLatDeviation" : standardLatDeviation,
		"standardLngDeviation" : standardLngDeviation,
		"meanLatDeviation" : meanLat,
		"meanLngDeviation" : meanLng
	};
	
}

function centerGravity()
{
	var center = null;
	
	var totalLat = 0.0;
	var totalLng = 0.0;
	var total = 0;
	
	for( var i in listingMarkers) {
		if( typeof i == 'string' ) {
			var marker = listingMarkers[i];
			var latLng = marker.getPosition();
			var lat = latLng.lat();
			var lng = latLng.lng();
			totalLat += lat;
			totalLng += lng;
			total++;
		}
	}
	
	var centerLat = totalLat/total;
	var centerLng = totalLng/total;
	center = new google.maps.LatLng( centerLat, centerLng );
	
	if( center != null ) {
		var marker = new google.maps.Marker( {
		   		position: center,
		   		map: gmap,
		   		flat: true,
		   		zIndex: 100,
		   		shape: {
